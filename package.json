{
  "name": "fume-fhir-converter",
  "version": "2.0.1",
  "description": "FHIR-Utilized Mapping Engine - Community",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "run": "npm run build && node dist/src/app.js",
    "dev": "npm run clean && cross-env NODE_ENV=dev nodemon -w src --exec \"npm run run\"",
    "start": "npm run clean && npm run run",
    "eslint:fix": "npx eslint --fix .",
    "test:debug": "node --inspect-brk --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.integration.js --runInBand",
    "prepare": "npm run build && husky"
  },
  "author": "Outburn Ltd.",
  "license": "AGPL-3.0",
  "keywords": [
    "FHIR",
    "FHIR Converter",
    "Mapper",
    "Mapping Engine",
    "HL7",
    "V2 to FHIR",
    "CSV to FHIR"
  ],
  "homepage": "https://fume.health",
  "repository": {
    "type": "git",
    "url": "https://github.com/Outburn-IL/fume-community"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "fhir-package-loader": "^0.2.0",
    "fs-extra": "^11.1.1",
    "hl7-dictionary": "^1.0.1",
    "hl7js": "^0.0.6",
    "js-sha256": "^0.9.0",
    "jsonata": "^2.0.2",
    "uuid-by-string": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "args": "^5.0.3",
    "cross-env": "^7.0.3",
    "detect-indent": "^7.0.1",
    "detect-newline": "^4.0.1",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "nodemon": "^2.0.20",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "simple-git": "^3.22.0",
    "stringify-package": "^1.0.1",
    "supertest": "^6.3.4",
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": "./dist/src/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "mjs,js,ts,json,env"
  }
}
