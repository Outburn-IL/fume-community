{
  "name": "fume-fhir-converter",
  "version": "3.0.0",
  "description": "FHIR-Utilized Mapping Engine - Community",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "Outburn Ltd.",
  "license": "AGPL-3.0",
  "keywords": [
    "FHIR",
    "FHIR Converter",
    "Mapper",
    "Mapping Engine",
    "HL7",
    "V2 to FHIR",
    "CSV to FHIR"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "run": "npm run build && node dist/app.cjs",
    "dev": "npm run clean && cross-env NODE_ENV=dev nodemon -w src --exec \"npm run run\"",
    "start": "npm run clean && npm run run",
    "serve-only": "npm run openapi && node dist/app.cjs",
    "openapi": "node -e \"require('fs').copyFileSync('src/openapi.yaml', 'dist/openapi.yaml')\"",
    "eslint:fix": "npx eslint --fix .",
    "test:debug": "node --inspect-brk --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.integration.cjs --runInBand",
    "test:http": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.http.integration.cjs --runInBand",
    "test:http:docker": "docker build --pull -t fume-community:test . && cross-env FUME_TEST_TARGET=docker FUME_TEST_DOCKER_IMAGE=fume-community:test npm run test:http",
    "test:stop-hapi": "cd tests && docker compose stop",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.integration.cjs --runInBand",
    "prepare": "npm run build",
    "prepublishOnly": "npm ci && npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://fume.health",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outburn-IL/fume-community.git"
  },
  "dependencies": {
    "@outburn/fhir-client": "^1.5.0",
    "@outburn/format-converter": "^2.0.1",
    "@outburn/fume-mapping-provider": "^1.1.1",
    "@outburn/structure-navigator": "^1.9.2",
    "@outburn/types": "^0.2.0",
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "fhir-package-explorer": "^1.9.2",
    "fhir-package-installer": "^1.11.3",
    "fhir-snapshot-generator": "^2.2.2",
    "fhir-terminology-runtime": "^1.2.1",
    "fs-extra": "^11.3.3",
    "fumifier": "^2.1.2",
    "js-yaml": "^4.1.1",
    "lru-cache": "^11.2.6",
    "openapi-types": "^12.1.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.23",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.3",
    "supertest": "^7.2.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "mjs,js,ts,json,env"
  }
}
