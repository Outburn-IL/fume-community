{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "pretty": true,
    "sourceMap": true,
    "outDir": "dist",
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "include": [
    "logic/**/*",
    "engine/**/*",
    "fume.ts"
, "communitySRC/types"    // ,"src/hl7.fhir.r4.core/StructureDefinition*.json"
, "logic/cache.ts", "logic/cacheHandler.ts", "logic/config.ts", "logic/logFunctions.ts", "logic/throwErrors.ts", "engine/hl7v2.ts", "engine/stringFunctions.ts", "engine/jsonataFunctions.ts", "engine/jsonataExpression.ts", "engine/objectFunctions.ts", "engine/parser.ts", "engine/fhirFunctions.ts"  ]
}
